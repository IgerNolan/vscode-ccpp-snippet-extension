	"class": {
		"prefix": "class",
		"body": [
			"\n#pragma region class",
			"// || -> enum --------------------- 40 || ? ------------------------------- 80 || ! ------------------------------ 120 |",
			"class ${1:ClassName} {",
			"    #pragma region class::enum",
			"    public:",
			"        $2",
			"    #pragma endregion class::enum",
			"",
			"    #pragma region class::struct",
			"    public:",
			"        $3",
			"    #pragma endregion class::struct",
			"",
			"    #pragma region class::union",
			"    public:",
			"        $4",
			"    #pragma endregion class::union",
			"",
			"    #pragma region class::class",
			"    public:",
			"        $5",
			"    #pragma endregion class::class",
			"",
			"    #pragma region class::const",
			"    public:",
			"        $6",
			"    #pragma endregion class::const",
			"",
			"    #pragma region class::function",
			"    #pragma region class::function::factory",
			"    public:",
			"        $7",
			"    #pragma endregion class::function::factory",
			"",
			"    #pragma region class::function::constructor",
			"    private:",
			"        ${1}() = default;",
			"    #pragma endregion class::function::constructor",
			"",
			"    #pragma region class::function::get/set",
			"    public:",
			"        $8",
			"    #pragma endregion class::function::get/set",
			"",
			"    #pragma region class::function::destructor",
			"    private:",
			"        ~${1}() = default;",
			"    #pragma endregion class::function::destructor",
			"",
			"    #pragma region class::function::interface",
			"    public:",
			"        $9",
			"    #pragma endregion class::function::interface",
			"",
			"    #pragma region class::function::inheritance",
			"    protected:",
			"        $10",
			"    #pragma endregion class::function::inheritance",
			"",
			"    #pragma region class::function::method",
			"    private:",
			"        $11",
			"    #pragma endregion class::function::method",
			"    #pragma endregion class::function",
			"",
			"    #pragma region class::variable",
			"    #pragma region class::variable::standard",
			"    private:",
			"        $12",
			"    #pragma endregion class::variable::standard",
			"",
			"    #pragma region class::variable::third",
			"    private:",
			"        $13",
			"    #pragma endregion class::variable::third",
			"",
			"    #pragma region class::variable::project",
			"    private:",
			"        $14",
			"    #pragma endregion class::variable::project",
			"    #pragma endregion class::variable",
			"}; // class ${1}",
			"#pragma endregion class\n"
		],
		"description": "Code snippet for 'class'"
	},
