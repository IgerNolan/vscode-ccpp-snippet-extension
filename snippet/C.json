{
	"#if": {
		"prefix": "#if",
		"body": [
			"\n#ifdef ${1}",
			"    $2",
			"#endif // $1\n"
		],
		"description": "Code snippet for '#if'"
	},
	"#ifn": {
		"prefix": "#ifn",
		"body": [
			"\n#ifndef ${1}",
			"    $2",
			"#endif // $1\n"
		],
		"description": "Code snippet for '#ifndef'"
	},
	"#inc<": {
		"prefix": "#inc<",
		"body": [
			"\n#include <$1>\n"
		],
		"description": "Code snippet for '#include <>'"
	},
	"#inc": {
		"prefix": "#inc",
		"body": [
			"\n#include \"$1\"\n"
		],
		"description": "Code snippet for '#include \"\"'"
	},
	"#def": {
		"prefix": "#def",
		"body": [
			"\n#define ($1) ($2)\n"
		],
		"description": "Code snippet for '#define'"
	},
	"enum": {
		"prefix": "enum",
		"body": [
			"\nenum ${EnumName}Enum {",
			"    $1",
			"}; // <- enum ${EnumName}Enum\n"
		],
		"description": "Code snippet for 'enum'"
	},
	"struct": {
		"prefix": "struct",
		"body": [
			"\nstruct ${StructName} {",
			"    $1",
			"}; // <- struct ${StructName}\n"
		],
		"description": "Code snippet for 'struct'"
	},
	"union": {
		"prefix": "union",
		"body": [
			"\nunion ${UnionName} {",
			"    $1",
			"}; // <- union ${UnionName}\n"
		],
		"description": "Code snippet for 'union'"
	},
	"if": {
		"prefix": "if",
		"body": [
			"\nif ($1) {",
			"    $2",
			"} // <- if $3\n"
		],
		"description": "Code snippet for 'if' statement"
	},
	"else": {
		"prefix": "else",
		"body": [
			"\nelse {",
			"    $1",
			"} // <- else $2\n"
		],
		"description": "Code snippet for 'else' statement"
	},
	"elif": {
		"prefix": "elif",
		"body": [
			"\nelse if ($1) {",
			"    $2",
			"} // <- elif $3\n"
		],
		"description": "Code snippet for 'elif' statement"
	},
	"switch": {
		"prefix": "switch",
		"body": [
			"\nswitch (${switch_on}) {",
			"    case ${case_1}:",
			"        $1",
			"        break;",
			"    default:",
			"        $2",
			"        break;",
			"} // <- switch $3\n"
		],
		"description": "Code snippet for 'switch' statement"
	},
	"while": {
		"prefix": "while",
		"body": [
			"\nwhile ($1) {",
			"    $2",
			"} // <- while $3\n"
		],
		"description": "Code snippet for 'while' loop"
	},
	"do": {
		"prefix": "do",
		"body": [
			"\ndo {",
			"    $1",
			"} while ($2); // <- do $3\n"
		],
		"description": "Code snippet for reverse 'do_while' loop"
	},
	"for": {
		"prefix": "for",
		"body": [
			"\nfor (${size_t} ${i} = ${1:0}; ${i} < ${2:length}; ${i}++) {",
			"    $3",
			"} // <- for $4\n"
		],
		"description": "Code snippet for 'for' loop"
	},
	"forr": {
		"prefix": "forr",
		"body": [
			"\nfor (int ${i} = ${1:length} - 1; ${i} >= ${2:0}; ${i}--) {",
			"    $3",
			"} // <- for $4\n"
		],
		"description": "Code snippet for reverse 'for' loop"
	},
