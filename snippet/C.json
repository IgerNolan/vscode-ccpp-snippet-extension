{
	"#if": {
		"prefix": "#if",
		"body": [
			"\n#ifdef ${1}",
			"    $2",
			"#endif // $1\n"
		],
		"description": "Code snippet for '#if'"
	},
	"#ifn": {
		"prefix": "#ifn",
		"body": [
			"\n#ifndef ${1}",
			"    $2",
			"#endif // $1\n"
		],
		"description": "Code snippet for '#ifndef'"
	},
	"#inc<": {
		"prefix": "#inc<",
		"body": [
			"\n#include <$1>\n"
		],
		"description": "Code snippet for '#include <>'"
	},
	"#inc": {
		"prefix": "#inc",
		"body": [
			"\n#include \"$1\"\n"
		],
		"description": "Code snippet for '#include \"\"'"
	},
	"#def": {
		"prefix": "#def",
		"body": [
			"\n#define ($1) ($2)\n"
		],
		"description": "Code snippet for '#define'"
	},
	"enum": {
		"prefix": "enum",
		"body": [
			"\n#pragma region enum",
			"enum ${1:EnumName}Enum {",
			"    $2",
			"}; // enum ${1}Enum",
			"#pragma endregion enum\n"
		],
		"description": "Code snippet for 'enum'"
	},
