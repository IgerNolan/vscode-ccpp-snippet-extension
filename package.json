{
    "name": "CCppSnippetExtension",
    "publisher": "IgerNolan",
    "displayName": "C/C++ Snippet Extension",
    "description": "Code snippets for C/C++",
    "version": "1.0.0",
    "engines": {
        "vscode": "^0.10.1"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "c",
                "path": "./snippets/c.json"
            },
            {
                "language": "cpp",
                "path": "./snippets/cpp.json"
            }
        ]
    },
    "galleryBanner": {
        "color": "#E6E0EA",
        "theme": "light"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/IgerNolan/vscode-ccpp-snippet-extension"
    },
    "icon": "img/Cpp.png",
    "license": "Check in License in the root folder"
}
